<!DOCTYPE html>
<html lang="en">
<script>
    var canvas = 'disabled',
            webGL = 'disabled';
    if (!!window.CanvasRenderingContext2D) {
        canvas = 'enabled';
    }

    if ((function () {
        try {
            return !!window.WebGLRenderingContext && !!document.createElement('canvas').getContext('experimental-webgl');
        }
        catch (e) {
            return false;
        }
    })()) {
        webGL = 'enabled'
    }

    window.location = "/triptychView?canvas=" + canvas + "&webGL=" + webGL;
</script>

</html>
